## Set to YES to disable ldconfig security checks
ldconfig_insecure="NO"

## ldconfig paths and directories
ldconfig_paths="/usr/lib/compat /usr/local/lib /usr/local/lib/compat/pkg"

ldconfig_paths_aout="/usr/lib/compat/aout /usr/local/lib/aout"

## ldconfig directories
ldconfig_local_dirs="/usr/lib/ldconfig /usr/local/lib/ldconfig"

## 32 bit ldconfig directories
ldconfig_local32_dirs="/usr/local/lib/ldconfig32"

